# VoltEdge Staging Environment Configuration

# Container Registry
REGISTRY=ghcr.io
IMAGE_NAME=voltedge

# Database Configuration
COCKROACH_PASSWORD=staging_voltedge_password_123
DATABASE_HOST=cockroachdb
DATABASE_PORT=26257
DATABASE_USER=voltedge
DATABASE_NAME=voltedge

# API Configuration
API_HOST=0.0.0.0
API_PORT=8080
LOG_LEVEL=debug

# Monitoring
GRAFANA_PASSWORD=staging_admin_password
PROMETHEUS_RETENTION=7d

# Security
JWT_SECRET=staging_jwt_secret_key_change_in_production
ENABLE_HTTPS=false

# Performance
MAX_CONCURRENT_SIMULATIONS=50
SIMULATION_TIMEOUT=5m
